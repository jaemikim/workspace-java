package kr.co.himedia.variable02;

public class BinaryTest {

	public static void main(String[] args) {
		
		int a = 10;
		float f = 35.6f;
		boolean  b = false;
		char c = 'A';
		String s = "APPLE";
		
		int decimal = 69;
		System.out.println("decimal = " + decimal);
		
		int binary = 0b01000101;
		System.out.println("decimal = " + binary);	
		
	}
}
