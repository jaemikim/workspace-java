public class ForTest04 {

	public static void main(String[] args) {
		
		// 2의 배수를 출력하시오
		// 초기식 생략
		int sum = 0;
		
//		for(int i = 0; i < 5; i++) {
//			
//			sum = i * 2;
//			
//			System.out.println(sum);
//		}
		
		for(; sum<10; sum++) {
			if(sum % 2 == 0) {
				System.out.println(sum + " ");
			}
		}
	}
}
