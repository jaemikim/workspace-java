package ko.co.himedia.operator;

public class ArithmeticOperTest {
	
	public static void main(String[] args) {
		
		int num1 = 7;
		int num2 = 3;
		int result;
		
		
		System.out.println("num1 + num2 = " + (num1 + num2));
		System.out.println("num1 - num2 = " + (num1 - num2));
		System.out.println("num1 * num2 = " + (num1 * num2));
		System.out.println("num1 / num2 = " + (num1 / num2));
		System.out.println("num1 % num2 = " + (num1 % num2));	
		
		// 예외 처리 코드
		try {
			
			result = 10 / 0;
		}

		
	}

}
