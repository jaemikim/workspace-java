package kr.co.himedia.loop04;

//1 ~ 100까지의 합을 구하시오
public class WhileTest {

	public static void main(String[] args) {
		
		int sum = 0; 
		int num = 1;  //while문은 초기화하는 부분이 따로 존재하지 않음
		
		while(num <= 100) {  //while문 옆에는 조건식만 들어감
			sum += num;
			num++;
		}
		
		System.out.println("1 ~ 100까지의 합 : " + sum);
	}
}
