package kr.co.himedia.loop02;

public class TriangleTest {

	public static void main(String[] args) {
	
		for(int i=1; i<=8; i++) {
			for(int j=1; j<=8; j++) {
			  System.out.print("*");				
				}
			  System.out.println();

			}	  	
		
		  System.out.println();
		
			for(int i=1; i<=8; i++) {
				for(int j=1; j<=8; j++) {
					
				if(j>i) {
				  System.out.print(" ");
				 } else {
				  System.out.print("*");
				 }
					}
				  System.out.println();

				}	
	}
}
